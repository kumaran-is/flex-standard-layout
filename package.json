{
  "name": "flex-standard-layout",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod-build": "ng build --prod --base-href=./",
    "dev-serve": "ng serve -o",
    "prod-serve": "ng serve --prod -o",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint && npm run app-version",
    "lint:styles": "stylelint ./src/**/*.scss",
    "scss-lint": "scss-lint",
    "e2e": "ng e2e",
    "outdated": "npm outdated --depth 0",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"flex-standard-layout documentation\"  --theme \"Vagrant\" -s -o ",
    "stats": "ng build --prod --base-href=./ --stats-json && webpack-bundle-analyzer dist/flex-standard-layout/stats.json",
    "conventional-changelog": "conventional-changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "git-publish": "npm run prod-build && npx angular-cli-ghpages --dir=dist/flex-standard-layout --branch=gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "~2.6.5",
    "rxjs": "~6.4.0",
    "tslib": "~1.9.0",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "~1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "angular-cli-ghpages": "~0.5.3",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "~2.0.11",
    "husky": "~1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "protractor": "~5.4.0",
    "source-map-explorer": "~1.7.0",
    "stylelint": "~9.10.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.13.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-language-service": "~0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "~3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "pre-push": "ng lint"
    }
  }
}
